<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context=".CoroutineStartPatternActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CoroutineStart"
        android:gravity="center"
        android:padding="50dp"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/start_default"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="DEFAULT"
        android:textAllCaps="false"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/start_atomic"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="ATOMIC"
        android:textAllCaps="false"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/start_lazy"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="LAZY"
        android:textAllCaps="false"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/start_undispatched"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="UNDISPATCHED"
        android:textAllCaps="false"
        />

</LinearLayout>